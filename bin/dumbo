#!/bin/sh

DUMBODIR="`dirname $0`/.."
HADOOPDIR="$DUMBODIR/../.."

if test -e "$HADOOPDIR/bin/hadoop"
then
  if test "$#" -ne "0"
  then
     PYTHONPATH="$DUMBODIR/lib/" python -m dumbo $@ -hadoop "$HADOOPDIR"
  else
     PYTHONPATH="$DUMBODIR/lib/" python -m dumbo
  fi
else
  python -m dumbo $@
fi
